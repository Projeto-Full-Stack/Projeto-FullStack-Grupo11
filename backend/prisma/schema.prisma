// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Announcement {
  id          String  @id
  color       String
  price       Int
  description String
  coverImage  String
  mileage     Int
  avaliable   Boolean

  @@map("announcements")
}
